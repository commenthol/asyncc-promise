{
  "name": "asyncc-promise",
  "version": "1.0.1",
  "description": "Just asynchronous patterns for promises",
  "keywords": [
    "async",
    "pattern",
    "promise"
  ],
  "homepage": "https://commenthol.github.io/asyncc-promise/",
  "bugs": {
    "url": "https://github.com/commenthol/asyncc-promise/issues"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "main": "dist",
  "jsnext:main": "src",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/asyncc-promise.git"
  },
  "scripts": {
    "build": "rollup -c scripts/rollup.conf.js",
    "clean": "rimraf lib docs coverage",
    "cover": "nyc --check-coverage --reporter=text --reporter=lcov npm run mocha",
    "dist": "npm run clean && npm run lint && npm run transpile && npm run build && npm run mocha && npm version",
    "doc": "jsdoc -c scripts/jsdoc/jsdoc.json",
    "karma": "karma start",
    "lint": "eslint --quiet '**/*.js'",
    "mocha": "mocha",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "npm run mocha",
    "transpile": "babel -d lib src"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ],
    "rules": {
      "spaced-comment": 0,
      "no-new": 0
    }
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "core-js": "^2.4.1",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "minami": "^1.2.3",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "rimraf": "^2.6.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-watch": "^4.0.0",
    "uglify-es": "^3.0.24"
  }
}
